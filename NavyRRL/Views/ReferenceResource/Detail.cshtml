@using Models.Schema;
@model ReferenceResource
@{
	ViewBag.Title = "Reference Resource: " + Model.Name;
}
<h2>Source</h2>
<h3>@Model.Name</h3>

<p>@Model.CodedNotation</p>
<p>Date of Source: @Model.PublicationDate</p>
<p>@Model.Description</p>
<p><a href="@Url.Content("~/" + Model.GetType().Name.ToLower() + "/edit/" + Model.Id)" target="_blank">Edit</a></p>